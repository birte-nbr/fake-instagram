<!DOCTYPE html>
<html lang="en">


<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css" />


</head>
<body>
    <h1>User Overview</h1>
    <% data.result.forEach(function(user) { %>

    <% if (data.result.length > 0) { %>
    <ul>
        <li>Username:
            <%= user.username %> 
        </li>


        <li>Name:    <%= user.given_name%>   <%=user.surname %>
        </li>

    </ul>
        <% } else { %>        <p>No user found</p>
            <% } %>
    <% }); %>


</body>
</html>
